                "str(destination + '/' + 'WA_Fn-UseC_-Telco-Customer-Churn.csv')  \n",
                "ChurnData = pd.read_csv(str(destination + '/' + 'WA_Fn-UseC_-Telco-Customer-Churn.csv'))"
